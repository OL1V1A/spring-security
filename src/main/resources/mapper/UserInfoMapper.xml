<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.lwj.springsecurity.dao.UserInfoDao">
    <select id="selectUserInfoByGtFraction" resultType="com.lwj.springsecurity.entity.UserInfoEntity" parameterType="long">
        select * from user_info where fraction > #{fraction}

    </select>
</mapper>